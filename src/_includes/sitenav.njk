<nav aria-labelledby="siteid" class="jlok-sitenav">
  <div class="jlok-sitenav__container">
    <a id="jlok-siteid" href="/" class="jlok-mr-sm">{{ meta.siteName }}</a>
    <ul class="jlok-sitenav__navlist jlok-list-unstyled">
      {%- for entry in collections.all | eleventyNavigation %}
        <li>
          <a href="{{ entry.url | url }}">{{ entry.title }}</a>
        </li>
      {%- endfor %}
    </ul>
  </div>
</nav>
